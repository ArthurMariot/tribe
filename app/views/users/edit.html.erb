  <h1>Personnal Details</h1>
  <p>Fill the form below to create your profil</p>
  <div>
    <%= simple_form_for @user do |f| %>
    <table>
      <tr>
      <%= f.input :avatar, label: 'Profile picture' %> </tr>
     <tr> Hobbies</tr>
     <tr> <%= f.input :hobby_1, label: 'Hobby 1' %>
      <%= f.input :hobby_2, label: 'Hobby 2' %>
      <%= f.input :hobby_3, label: 'Hobby 3' %> </tr>
      <%= f.input :location, label: 'Location' %>
      <%= f.input :linkedin_url, label: 'Linkedin url' %>
      <%= f.input :slack_account, label: 'Slack account' %>
      <%= f.button :submit, class:"btn-green" %>
    <% end %>
    </table>
  </div>

